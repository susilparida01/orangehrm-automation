<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" verbose="1" parallel="tests" thread-count="3">
  <listeners>
	 <listener class-name="com.orangehrm.listeners.TestListener"/>
	 <listener class-name="com.orangehrm.listeners.RetryTransformer"/>
  </listeners>

  <parameter name="browser" value="chrome"/>
  <parameter name="headless" value="false"/>

  <test thread-count="5" name="Test">
    <classes>
      <class name="com.orangehrm.tests.Login_AdminUser"/>
      <class name="com.orangehrm.tests.BaseTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
